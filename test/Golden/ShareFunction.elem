foreign export "value" pureValue : IO (∀ 0 → 0)

foreign primitive pureIO : ∀ 0 → IO 0
foreign primitive bindIO : ∀ IO 0 → ∀ (1 → IO 0) → IO 0

pureValue = bindIO @(∀ 0 → 0) (pureIO @(∀ 0 → 0) value)
    @(∀ 0 → 0) (λ(∀ 0 → 0) pureIO @(∀ 0 → 0) 0)

value = Λ λ0 0

-- This assignment causes duplication, which can break the rewriting.
break = value

